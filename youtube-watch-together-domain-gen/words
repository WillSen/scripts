var threeLetterWords = "AbyAnyBayBeyBoyBuyCayCoyCryDayDeyDryFayFeyFlyFoyFryGayGeyGoyGuyHayHeyHoyIcyIvyJayJoyKayKeyLayLeyMayNayOxyPayPlyPryRaySayShySkySlySoySpyStyThyToyTryWayWhyWryYay"

var threeLetterWordsOutput = [];

for (var i = 0; i < threeLetterWords.length; i += 2) {
  var localword = threeLetterWords[i] + threeLetterWords[i+1];
  threeLetterWordsOutput.push(localword)
};

console.log(threeLetterWordsOutput)